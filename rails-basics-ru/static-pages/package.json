    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.2",
    "@popperjs/core": "^2.11.7",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "esbuild": "^0.20.0",
    "postcss-import": "^16.0.0",
    "tailwindcss": "^3.4.1"
  }
}
